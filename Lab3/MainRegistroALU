module MainRegistroALU #(parameter n = 8) (
        input logic [2*n+4:0] Q,
        input logic CLK,
        input logic RST,
        output logic [2*n+3:0] D

    );
    logic [3:0] ALUFlags;
    logic  [n-1:0]ALUResult;
    logic [n+3:0] Q2;
    logic [n+3:0] D2;
    RegistroALU #(n) R1(Q, CLK, RST, D);
    AluPrueba #(n) alu(D[n-1:0], D[2*n-1:n], D[2*n+4], D[2*n+3:2*n],ALUFlags,ALUResult);
    
    RegistroALU #(n) R2();
endmodule
